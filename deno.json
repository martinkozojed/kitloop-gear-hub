{
  "lock": "deno.lock",
  "nodeModulesDir": "none",
  "tasks": {
    "lock": "deno cache --lock=deno.lock --lock-write supabase/functions/**/*.ts scripts/**/*.ts",
    "deno:cache": "deno cache --lock=deno.lock --frozen supabase/functions/**/*.ts scripts/**/*.ts",
    "deno:test": "deno test --allow-env --allow-read --lock=deno.lock --frozen supabase/functions"
  }
}
